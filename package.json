    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/ioredis": "^4.0.4",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.119",
